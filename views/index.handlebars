<div class="container">
    {{#each article}}
    <div class="card">
        <h5 class="card-header newsTitle"><a class="article-link" target="_blank" href={{link}}>{{title}}</a></h5>
        <div class="card-body">
            <!-- <h5 class="card-title">Special title treatment</h5> -->
            <p class="card-text newsDescription"></p>
            <a href="#" class="btn btn-primary saveBtn">Save</a>
        </div>
    </div>
    {{/each}}
</div>